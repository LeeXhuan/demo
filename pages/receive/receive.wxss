/* pages/receive/receive.wxss */
#l-receive{
    position: absolute;
    top: 0;
    right: 0;
    background: #212526;
    width: 100%;
    height: 100%;
    display: flex;
}
#l-receive .l-container{
    box-sizing: border-box;
    width: 616rpx;
    height: 792rpx;
    background-color: #fff;
    margin: 100rpx auto 0;
    border-radius: 4rpx;
    padding: 48rpx 0 0 64rpx;
}
#l-receive .l-container .l-title{
    font-size: 36rpx;
    color: #202728;
    line-height: 48rpx;
}