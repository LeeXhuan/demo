<!--pages/notice/notice.wxml-->
<view id='l-notice'>
  <view class='l-container'>
    <view class="l-title">车票需知</view>
    <view class="l-container-text">
        <view class="l-text" wx:for="{{noticeList}}" wx:for-item="item" wx:key="index">{{index+1}}、{{item}}</view>
    </view>
    <view class="l-footer" bindtap="return">返回</view>
  </view>
</view>
